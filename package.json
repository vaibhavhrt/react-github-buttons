    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "lint": "tslint **/src/**.{ts,tsx} --exclude **/node_modules/**"
  },
  "dependencies": {},
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^15.0.0 || ^16.0.0",

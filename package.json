    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "eslint": "^7.10.0",
    "gh-pages": "^2.0.1",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^1.15.2",
    "rollup-plugin-babel": "^4.3.2",
